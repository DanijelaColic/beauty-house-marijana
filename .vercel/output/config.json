{
	"version": 3,
	"routes": [
		{
			"handle": "filesystem"
		},
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"src": "^/_server-islands/([^/]+?)/?$",
			"dest": "_render"
		},
		{
			"src": "^/_image/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/availability/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/book/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/services/?$",
			"dest": "_render"
		},
		{
			"src": "^/rezervacije/uspjeh/?$",
			"dest": "_render"
		},
		{
			"src": "^/rezervacije/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	]
}